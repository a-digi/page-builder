    },
    "peerDependencies": {
        "react": "^19.0.0-rc.0",
        "react-dom": "^19.0.0-rc.0"
    },
    "devDependencies": {
        "@types/node": "^20.12.7",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@typescript-eslint/eslint-plugin": "^7.2.0",
        "@typescript-eslint/parser": "^7.2.0",
        "@vitejs/plugin-react": "^4.3.1",
        "autoprefixer": "^10.4.19",
