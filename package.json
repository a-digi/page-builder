{
    "name": "@a-digi/page-builder",
    "private": false,
    "version": "1.6.3",
    "description": "A flexible page builder component for React.",
    "author": "Your Name <your.email@example.com>",
    "license": "MIT",
    "homepage": "https://github.com/a-digi/page-builder",
    "repository": {
        "type": "git",
        "url": "https://github.com/a-digi/page-builder.git"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/"
    },
    "keywords": [
        "react",
        "typescript",
        "page-builder",
